{
  "name": "APP_TEMPLATE",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "watchman watch-del-all",
    "android": "yarn clean && ENVFILE=.env react-native run-android",
    "build:android": "yarn clean && cd android && ./gradlew clean && ENVFILE=.env ./gradlew :app:assembleRelease && cd app/build/outputs/apk/release/ && open .",
    "bundle:android": "yarn clean && cd android && ./gradlew clean && ENVFILE=.env ./gradlew :app:bundleRelease && cd app/build/outputs/bundle/release/ && open .",
    "ios": "yarn clean && ENVFILE=.env react-native run-ios",
    "lint": "eslint .",
    "start": "yarn clean && ENVFILE=.env react-native start --reset-cache --client-logs",
    "test": "jest",
    "postinstall": "cd ios && bundle exec pod install && cd .."
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.8.12",
    "@react-navigation/native": "^7.1.25",
    "@react-navigation/native-stack": "^7.8.6",
    "@reduxjs/toolkit": "^2.11.1",
    "decimal.js": "^10.6.0",
    "i18next": "^25.7.3",
    "i18next-icu": "^2.4.1",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.6.0",
    "react": "19.1.1",
    "react-hook-form": "^7.68.0",
    "react-i18next": "^16.5.0",
    "react-native": "0.82.1",
    "react-native-calendars": "^1.1313.0",
    "react-native-config": "^1.6.1",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-image-crop-picker": "^0.51.1",
    "react-native-reanimated": "^4.2.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-splash-view": "^0.0.19",
    "react-native-worklets": "^0.7.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "sharetribe-flex-sdk": "^1.22.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.21",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
